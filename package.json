  "scripts": {
    "start": "react-scripts --max_old_space_size=4096 start",
    "build": "react-scripts --max_old_space_size=4096 build && electron-builder --publish always",
    "test": "react-scripts test",
    "start:electron": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync build/ s3://www.ezygentechnology.in",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dev": "cross-env NODE_ENV=development electron .",
    "rebuild-app": "electron-builder install-app-deps",
    "start-electron-app": "concurrently -k \"cross-env BROWSER=none npm run start\" \"cross-env NODE_ENV=development  npm run electron\"",
    "package-windows": "rm -rf build && rm -rf dist && npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "package-mac": "rm -rf build && rm -rf dist && npm run build && electron-builder build  -m -c.extraMetadata.main=build/electron.js ",
    "package-linux": "rm -rf build && rm -rf dist && npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "publish": "webpack --config=build/webpack.app.config.js --env=production && electron-builder --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.1",
    "electron-builder": "^24.4.0",
    "wait-on": "^7.0.1"
  },
  "repository": "https://github.com/sayyedsiddique/bluebill-autoupdate-electron",
  "build": {
    "appId": "ezygen.blubill.app",
    "productName": "BlueBillPOS",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "sayyedsiddique",
        "repo": "bluebill-autoupdate-electron"
      }
    ],
    "directories": {
      "buildResources": "public"
    },
    "extraResources": [
      "./bbdatabase.db"
    ]
  }
